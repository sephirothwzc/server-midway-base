{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "quotes": [
      2,
      "single"
    ],
    "strict": 0,
    "no-shadow": 0,
    "no-undef": 0,
    "prefer-arrow-callback": 0,
    "no-console": 1,
    "no-var": 0,
    "no-magic-numbers": 0,
    "eqeqeq": 0,
    "prefer-template": 0,
    "no-use-before-define": 0,
    "max-params": 0,
    "max-len": 0,
    "max-nested-callbacks": 0,
    "no-catch-shadow": 0,
    "max-depth": 0,
    "no-redeclare": 0,
    "max-statements": 0,
    "new-cap": 0,
    "no-lonely-if": 0,
    "global-require": 0,
    "complexity": [
      2,
      20
    ],
    "no-process-exit": 0
  }
}